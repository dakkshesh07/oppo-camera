<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="10.0dip" />
            <gradient android:startColor="#ffb8b8b8" android:endColor="#ffb8b8b8" android:angle="0.0" android:type="linear" android:centerY="0.5" android:centerColor="#ffb8b8b8" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="10.0dip" />
                <gradient android:startColor="#ffff5500" android:endColor="#ffff5500" android:angle="0.0" android:centerX="1.0" android:centerColor="#ffff5500" />
            </shape>
        </clip>
    </item>
</layer-list>